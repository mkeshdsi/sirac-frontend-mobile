{
  "expo": {
    "name": "SIRAC",
    "slug": "sirac",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sirac",
    "userInterfaceStyle": "automatic",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to get the position of the banca.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to get the position of the banca."
      }
    },
    "android": {
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "output": "static"
    }
  }
}