{
  "expo": {
    "name": "SIRAC",
    "slug": "sirac",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sirac",
    "userInterfaceStyle": "automatic",
    "icon": "./logo_png.png",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/1d5f13c3-18c6-418b-ad75-9a41e922dc29"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.eleuterio.sirac",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs access to location to get the position of the banca.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs access to location to get the position of the banca."
      }
    },
    "android": {
      "package": "com.eleuterio.sirac",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "1d5f13c3-18c6-418b-ad75-9a41e922dc29"
      }
    }
  }
}